h3
  height 30px
  margin-bottom 22px
  line-height 30px
  padding-left 8px
  border-bottom 1px solid #bababa
  color #bababa

main
  article
    overflow hidden
    aside
      float left
      text-align center
      color #666
      ul
        li
          width 240px
          height 40px
          line-height 40px
          font-size 20px
          &:hover
            background #01af63
            a
              color #fff
    section
      width 885px
      min-height 270px
      padding-left 32px
      margin-bottom 108px
      float right
      border-left 1px solid #ccc
      h2
        height 55px
        line-height 55px
        color #333
        font-size 20px
      p
        line-height 25px

