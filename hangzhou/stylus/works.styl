h3
  height 30px
  margin-bottom 22px
  line-height 30px
  padding-left 8px
  border-bottom 1px solid #bababa
  color #bababa

main
  article
    overflow hidden
    aside
      float left
      text-align center
      ul
        li
          width 240px
          height 40px
          line-height 40px
          font-size 20px
          a
            color #666
          &:hover
            background #01af63
            a
              color #fff
    section
      width 885px
      min-height 270px
      padding-left 32px
      margin-bottom 108px
      float right
      border-left 1px solid #ccc
      h2
        font-size 18px
        text-align center
      div
        margin-top 10px
        text-align center
        span
          margin-right 30px
          color #666
        input[type="button"]
          width 80px
          height 25px
          line-height 25px
          background #096
          color #fff
          border-radius 5px
          font-size 14px
          outline none
     dl
       font-size 14px
       color #333
       dt,dd
         line-height 25px

